@*
    
*@
@{
    Layout = "_Layout2";
    ViewData["Title"] = "Details";  
}

@model PE2023test.Models.Fine

<section class ="breadcrumb">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2>Fine Details Page.</h2>
            </div>
        </div>
    </div>
</section>

<section class="fine-details">
    <div class = "container">
        <div class="row row-balance">
            <div class="col-lg-12">
                <div class="title-text">
                    <h4>Fine Details</h4>
                </div>
           </div>
        </div>
        <div class="row">
            <dl class="dl-horizontal">
                <dt>  
                    @Html.DisplayNameFor(model => model.SessionID)  
                </dt>  
                <dd>  
                    @Html.DisplayFor(model => model.SessionID)  
                </dd>  
                <dt>  
                    @Html.DisplayNameFor(model => model.Fine_days)  
                </dt>  
                <dd>  
                    @Html.DisplayFor(model => model.Fine_days)  
                </dd>  
                <dt>  
                    @Html.DisplayNameFor(model => model.Fine_amount)  
                </dt>  
                <dd>  
                    @Html.DisplayFor(model => model.Fine_amount)  
                </dd> 
            </dl>  
        </div>
    </div>
    <div class="row link-row">
        <div class="col-lg-4">
             <a asp-action="FineEdit" asp-route-id="@Model.SessionID" class="navigate-link">Edit</a> |  
        </div>
        <div class="col-lg-4">
               <a asp-action="FineIndex" class="navigate-link">Back to List</a>  
        </div>
    </div>
</section>
